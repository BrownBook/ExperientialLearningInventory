<div class="nq-container">
  <!-- BEGIN NOTIFICATIONS -->
  <!-- BEGIN ERROR -->
  <div class="alert alert-danger alert-dismissible fade show">
    <i class="fa-solid fa-circle-xmark fa-lg"></i> {ERROR}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <!-- END ERROR -->

  <!-- BEGIN WARNING -->
  <div class="alert alert-warning alert-dismissible fade show">
    <i class="fa-solid fa-triangle-exclamation fa-lg"></i> {WARNING}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <!-- END WARNING -->

  <!-- BEGIN SUCCESS -->
  <div class="alert alert-success alert-dismissible fade show">
    <i class="fa-solid fa-check fa-lg"></i> {SUCCESS}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <!-- END SUCCESS -->

  <!-- BEGIN UNKNOWN -->
  <div class="alert alert-info alert-dismissible fade show">
    <i class="fa fa-circle-info fa-lg"></i> {UNKNOWN}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <!-- END UNKNOWN -->
  <!-- END NOTIFICATIONS -->
</div>